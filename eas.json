{
  "cli": {
    "version": ">= 16.20.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "RCT_USE_RN_DEP": "0",
        "RCT_USE_PREBUILT_RNCORE": "0"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "RCT_USE_RN_DEP": "0",
        "RCT_USE_PREBUILT_RNCORE": "0"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
